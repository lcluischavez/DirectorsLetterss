<div class="contact-container">

    
    <div class="contact-img">
        <img src="/images/maleconductor.jpg" alt="contact_image_director">
    </div>
    <div class="contact-form">
        <div class="form-header">
            <h3>Feel free to contact us</h3>
            <h5>Have comments, questions, feedback or share? 
                Our team woulds love to here from you
            </h5>
        </div>
        <div class="form">
        <div class="contact-row">
            <label for="name"></label>
            <input type="text" placeholder="Name">
            <label for="subject"></label>
            <input type="text" placeholder="Subject">
        </div>
        <div class="email">
            <label for="email"></label>
            <input type="text" placeholder="Email">
        </div>
        <div class="msg">
            <label for="msg"></label>
            <textarea name="msg" id="msg" cols="43" rows="6" placeholder="Message"></textarea>
        </div>
        </div>

    </div>
</div>

</div>

